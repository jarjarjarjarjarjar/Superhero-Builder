        new_image("hulk_legs.png")
    }
    if (my_keydown() == '76' && my_keydown() == '73') {
        new_image("ironman_legs.png")
    }
    if (my_keydown() == '70' && my_keydown() == '83') {
        new_image("spiderman_face.png")
    }
    if (my_keydown() == '70' && my_keydown() == '72') {
        new_image("hulk_face.png")
