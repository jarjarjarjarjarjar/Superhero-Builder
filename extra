if (e.shiftkey == true && my_keydown() == '73') {
        height = height + 10
        width = width + 10
    }
    if (e.shiftkey == ture && my_keydown() == '68') {
        height = height - 10
        width = width - 10
    }
    if (my_keydown() == '66' && my_keydown() == '83') {
        new_image('spiderman_body.png')
    }
    if (my_keydown() == '66' && my_keydown() == '72') {
        new_image('hulk_body.png')
    }
    if (my_keydown() == '66' && my_keydown() == '73') {
        new_image('ironman_body.png')
    }
    if (my_keydown() == '76' && my_keydown() == '83') {
        new_image("spiderman_legs.png")
    }
